    /* Estilos para a sub-tela */
    .sub-tela {
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background-color: #2079b0(0, 0, 0, 0.5);
        /* Fundo semi-transparente */
        z-index: 9999;
        /* Nível de sobreposição para aparecer acima do conteúdo principal */
        display: none;
        /* Inicialmente oculta */
    }

    .sub-tela-conteudo {
        position: absolute;
        top: 50%;
        left: 50%;
        transform: translate(-50%, -50%);
        background-color: #fff;
        padding: 30px;
        width: 600px;
        max-width: 90%;
        border-radius: 8px;
        box-shadow: 0 2px 6px #2079b0(0, 0, 0, 0.3);
        text-align: center;
        background-color: #7BC8FF;
        border: 4px solid #2079b0;

