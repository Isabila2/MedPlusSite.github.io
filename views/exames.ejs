    // Função para abrir a sub-tela
    function abrirSubTela() {
        document.getElementById("subTela").style.display = "block";
    }

    // Função para fechar a sub-tela
    function fecharSubTela() {
        document.getElementById("subTela").style.display = "none";
    }

    // Função para verificar a resposta
    function verificarResposta() {
        var resposta = document.querySelector('input[name="resposta"]:checked');

        if (resposta) {
            if (resposta.value === "sim") {
                // Ação a ser realizada caso a resposta seja "Sim"
                alert("Exame confirmado!");
                fecharSubTela();
            } else if (resposta.value === "nao") {
                // Ação a ser realizada caso a resposta seja "Não"
                alert("Exame não confirmado.");
                fecharSubTela();
            }
        } else {
            alert("Por favor, selecione 'Sim' ou 'Não'."); // Alerta para resposta não selecionada
        }
    }
</script>

